{
  "applications": {
    "XoaiSay.HttpApi.Host": {
      "type": "dotnet-project",
      "launchUrl": "https://localhost:44352",
      "path": "../../../src/XoaiSay.HttpApi.Host/XoaiSay.HttpApi.Host.csproj",
      "kubernetesService": ".*-httpapihost$",
      "healthCheckEndpoint": "/health-status",
      "healthUiEndpoint": "/health-ui",
      "execution": {
        "order": 4
      }
    },
    "XoaiSay.Angular": {
      "type": "cli",
      "workingDirectory": "../../../angular",
      "startCommand": "./start.ps1",
      "launchUrl": "http://localhost:4200",
      "kubernetesService": ".*-angular$",
      "execution": {
        "order": 2
      }    
    }
  },
  "containers": {
    "serviceName": "XoaiSay-Containers",
  }
}